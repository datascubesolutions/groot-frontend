{
  "name": "groot-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit || echo 'TypeScript not configured'",
    "test": "echo 'Tests not configured yet' && exit 0",
    "test:watch": "echo 'Tests not configured yet' && exit 0",
    "test:coverage": "echo 'Tests not configured yet' && exit 0",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4"
  }
}
